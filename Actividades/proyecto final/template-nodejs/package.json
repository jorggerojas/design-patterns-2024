{
  "name": "example-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "cleanup": "npm run format; npm run lint",
    "watch": "webpack --watch --config webpack.config.js",
    "build": "webpack",
    "dev": "docker compose --env-file .environment.local up app",
    "start": "docker compose --env-file .environment.local up app-build",
    "dbmate": "docker compose --env-file .environment.local run --rm --entrypoint dbmate migrations",
    "dbmate:seed": "docker compose --env-file .environment.local run --rm --entrypoint dbmate -e DBMATE_MIGRATIONS_DIR=db/seeds -e DBMATE_MIGRATIONS_TABLE=seed_migrations -e DBMATE_NO_DUMP_SCHEMA=1 migrations",
    "migrations": "docker compose --env-file .environment.local run --rm migrations"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cerealizr": "^1.0.1-alpha",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "hot-shots": "^9.0.0",
    "inversify": "^6.0.1",
    "ioredis": "^4.28.3",
    "joi": "^17.6.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "nodemon": "^2.0.15",
    "nodemon-webpack-plugin": "^4.0.8",
    "prettier": "^2.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^1.7.2"
  }
}
